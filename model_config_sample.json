{
    "model" :{
        "num_subtypes" : 2,
        "base_model" : "resnet18",
        "pretrained" : false,
        "last_layers": "short",
        "concat_pool": true
    },
    "normalize" : {
        "use_normalize" : true,
        "mean" : [ 0.7371, 0.6904, 0.8211 ],
        "std" : [ 0.0974, 0.0945, 0.0523 ]
    },
    "augmentation" : {
        "use_augmentation": true,
        "flip": true,
        "color_jitter": true,
        "rotation": true,
        "cut_out": {
            "num_cut": 2,
            "size_cut": 100,
            "color_cut": "white"
        }
    },
    "use_weighted_loss" : {
        "use_weighted_loss" : false,
        "weight": [2, 1]
    },
    "use_weighted_sampler" : false,
    "use_balanced_sampler" : true,
    "mix_up" : {
        "use_mix_up" : true,
        "alpha": 0.4
    },
    "freeze": -1,
    "continue_train": false,
    "optimizer" : {
        "type" : "Adam",
        "parameters" : {
            "lr" : 0.00001,
            "amsgrad" : true,
            "weight_decay" : 0.0005
        }
    },
    "scheduler" : {
        "type" : "OneCycleLR",
        "parameters" : {
            "max_lr" : 0.01,
            "steps_per_epoch" : 1202,
            "epochs" : 10,
            "three_phase": true
        }
    }
}
